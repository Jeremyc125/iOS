import UIKit
//loop through 1-100
for i in 1...100 {
    //if divisible by 3 but not 15
    if(i%3==0 && i%15 != 0){
        print("Fizz")
        //if divisible by 5 and not 15
    }else if(i%5==0 && i%15 != 0){
        print("Fuzz")
        //if divisible by 15
    }else if(i%15 == 0){
        print("Fizz Buzz")
        //everything else
    }else{
        print(i)
    }
    
}
